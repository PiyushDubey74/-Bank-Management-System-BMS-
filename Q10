#include <iostream>
using namespace std;

void Palindrome(int num) {
    int a = num;
    int rev = 0;

    while (num != 0) {
        int digit = num % 10;
        rev = rev * 10 + digit;
        num = num / 10;
    }

    if (a == rev)
        cout << a << " is palindrome";
    else
        cout << a << " is not palindrome";
}

int main() {
    int number;
    cout << "Enter a number: ";
    cin >> number;
    Palindrome(number);
}
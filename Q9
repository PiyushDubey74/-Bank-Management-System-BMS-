#include <iostream>
using namespace std;

int main() {
    int a[10];
    int min, max;

    cout << "Enter elements: ";
    for (int i = 0; i < 10; i++) {
        cin >> a[i];
    }

    min = a[0];
    max = a[0];

    for (int i = 1; i < 10; i++) {
        if (a[i] < min)
            min = a[i];
        else if (a[i] > max)
            max = a[i];
    }

    cout << "Minimum = " << min << endl;
    cout << "Maximum = " << max;
}
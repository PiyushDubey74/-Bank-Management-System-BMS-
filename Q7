#include <iostream>
using namespace std;

int main() {
    int a[10];
    int x;
    int f = 0;

    cout << "Enter 10 elements: ";
    for (int i = 0; i < 10; i++) {
        cin >> a[i];
    }

    cout << "Enter element to search: ";
    cin >> x;

    for (int i = 0; i < 10; i++) {
        if (a[i] == x) {
            f = 1;
            break;
        }
    }

    if (f == 1)
        cout << "Element found";
    else
        cout << "Element not found";
}
#include <iostream>
using namespace std;

int main() {
    int N;
    int sum = 0;
    double avg;

    cout << "Enter N: ";
    cin >> N;

    for (int i = 1; i <= N; i++) {
        cout << i << " ";
        sum += i;
    }

    avg = (double)sum / N;

    cout << "Sum = " << sum;
    cout << "Average = " << avg;
}